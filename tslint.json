{
  "rulesDirectory": [
    "tslint-plugin-prettier"
  ],
  "extends": [
    "tslint-react"
  ],
  "env": {
    "browser": true,
    "jest": true
  },
  "rules": {
    "prettier": true,
    "semicolon": [
      true,
      "always",
      "ignore-bound-class-methods"
    ],
    "jsx-no-multiline-js": false,
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "no-empty-interface": false,
    "member-access": false,
    "strict-boolean-expressions": false,
    "variable-name": false
  }
}
